[ignore]
<PROJECT_ROOT>/buildtools/.*
<PROJECT_ROOT>/doc/.*
<PROJECT_ROOT>/errorShots/.*
<PROJECT_ROOT>/logs/.*
<PROJECT_ROOT>/project/.*
<PROJECT_ROOT>/target/.*
<PROJECT_ROOT>/test/db/.*
<PROJECT_ROOT>/userBinaryData/.*
<PROJECT_ROOT>/binaryData/.*
<PROJECT_ROOT>/conf/.*
<PROJECT_ROOT>/lib/.*
<PROJECT_ROOT>/public/.*
<PROJECT_ROOT>/test/.*
<PROJECT_ROOT>/tools/.*
<PROJECT_ROOT>/node_modules/flow-coverage-report/
<PROJECT_ROOT>/node_modules/draft-js/
<PROJECT_ROOT>/app/assets/.*\.spec\.js

[options]
module.ignore_non_literal_requires=true
module.name_mapper='jasny-bootstrap' -> '<PROJECT_ROOT>/node_modules/jasny-bootstrap/dist/js/jasny-bootstrap.js'
module.name_mapper='backbone\.marionette' -> '<PROJECT_ROOT>/node_modules/backbone\.marionette/lib/backbone.marionette.js'
module.name_mapper='three' -> '<PROJECT_ROOT>/node_modules/three.js/build/three.js'
module.name_mapper='three.color' -> '<PROJECT_ROOT>/node_modules/three.js/examples/js/math/ColorConverter'
module.name_mapper='three.trackball' -> '<PROJECT_ROOT>/node_modules/three.js/examples/js/controls/TrackballControls'

module.name_mapper='^\([^.].*\)' -> '<PROJECT_ROOT>/app/assets/javascripts/\1'
module.name_mapper.extension='less' -> '<PROJECT_ROOT>/app/assets/javascripts/flow/LESSFlowStub.js.flow'

[libs]
app/assets/javascripts/flow/custom-libdefs
