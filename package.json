    "es6-promise": "^3.0.2",
    "file-saver": "^1.3.3",
    "font-awesome": "^4.5.0",
    "hammerjs": "^2.0.8",
    "history": "^4.7.2",
    "immutability-helper": "^2.6.0",
    "lodash": "^4.13.1",
    "mjs": "^1.0.0",
    "moment": "^2.19.3",
    "pako": "^0.2.8",
    "prop-types": "https://github.com/scalableminds/prop-types",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-google-charts": "^1.5.5",
    "react-redux": "^5.0.6",
    "react-remarkable": "^1.1.3",
    "react-router-dom": "^4.2.2",
