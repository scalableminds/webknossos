    "stats.js": "^1.0.0",
    "three": "^0.84.0",
    "tween.js": "^16.3.1",
    "whatwg-fetch": "^1.1.0"
  },
  "ava": {
    "source": [
      "./public/test-bundle/**/*.{js,jsx}"
    ],
    "snapshotDir": "app/assets/javascripts/test/snapshots",
